                        <option value="الماليزية">الماليزية</option>
                    </select>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-6">
                <div class="mt-3">
                    <label class="form-label">الجنس</label>
                    <select class="form-control" asp-for="Gender">
                        <option value="">اختر الجنس</option>
                        <option value="ذكر">ذكر</option>
                        <option value="أنثى">أنثى</option>
                    </select>
                </div>
            </div>

            <div class="col-6">
                <div class="mt-3">
                    <label class="form-label">الجهات</label>
                    <select class="form-control" asp-for="Entities">
                        <option value="">اختر الجهة</option>
