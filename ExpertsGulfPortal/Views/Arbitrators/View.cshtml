@{
}
@model ExpertsGulfPortal.Models.Entities.Arbitrators

<h1 dir="rtl">تفاصيل المحكم</h1>

<div class="mt-3" dir="rtl">
    <ul class="list-group">
        <li class="list-group-item"><strong>الجنسية:</strong> @Model.Nationality</li>
        <li class="list-group-item"><strong>اللغة المفضلة:</strong> @Model.PreferredLanguage</li>
        <li class="list-group-item"><strong>الجنس:</strong> @Model.Gender</li>
        <li class="list-group-item"><strong>الجهات:</strong> @Model.Entities</li>
        <li class="list-group-item"><strong>الدولة:</strong> @Model.Country</li>
        <li class="list-group-item"><strong>المدينة:</strong> @Model.City</li>
        <li class="list-group-item"><strong>الدرجة العلمية:</strong> @Model.AcademicDegree</li>
        <li class="list-group-item"><strong>التخصص العام:</strong> @Model.GeneralSpecialization</li>
        <li class="list-group-item"><strong>التخصص الدقيق:</strong> @Model.SpecificSpecialization</li>
        <li class="list-group-item"><strong>موضوع رسالة الماجستير:</strong> @Model.MastersThesisTitle</li>
        <li class="list-group-item"><strong>موضوع رسالة الدكتوراه:</strong> @Model.DoctoralThesisTitle</li>
    </ul>
</div>

<div class="text-center mt-4">
    <a asp-action="List" class="btn btn-primary btn-lg">رجوع إلى القائمة</a>
</div>
